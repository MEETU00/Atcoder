a,b,c,k=map(int,input().split())
if(k<=a):
  print(k)
else:
  print(a-(k-a-b))



a, b, c, k = map(int, input().split())
if k<=a:
    print(k)
elif a<k<=a+b:
    print(a)
else:
    print(a-(k-a-b))


a,b,c,k = map(int,input().split())
if a >= k:
    print(k)
elif (a+b) >= k:
    print(a)
else:
    print(a-(k-a-b))

