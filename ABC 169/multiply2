n = int(input())
a = list(map(int, input().split()))
 result = 1
 if 0 in a:
    print(0)
    exit()
 for i in range(n):
    result *= a[i]
    if result > 10**18:
        print(-1)
        exit()
print(result

n = int(input())
 
nums = list(map(int, input().split()))
 product = 1
for num in nums:
    product *= num
 
    if product > 1e18:
        product = -1
        break
 if 0 in nums:
    product = 0
 print(product)


n = list(map(int,input().split()))
A = list(map(int,input().split()))
A.sort()
ans = 1
for a in A:
  ans  = ans * a
  if ans > 10**18:exit(print(-1))
 print(ans)


