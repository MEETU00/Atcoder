x = input()
if x[0]=="7" or x[1]=="7" or x[2]=="7":
    ans = "Yes"
else:
    ans = "No"
print(ans)
**************************
N = input()
ans = "No"
for s in N:
    if s == "7":
        ans = "Yes"
        break
print(ans)
**************************
print('Yes' if '7' in input() else 'No')

**************************
A = input()
for a in range(3):
  if A[a] == '7':
    print('Yes')
    break
  if a == 2:
    print('No')
************************
